<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.mjbudden.android.solarsystem"
    android:versionCode="2"
    android:versionName="0.4"
    android:installLocation="preferExternal">
    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="8">
    </uses-sdk>
    <!--
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    -->
    <application
        android:label="@string/app_name"
        android:icon="@drawable/app_icon"
        android:theme="@android:style/Theme.NoTitleBar">
        <activity
            android:name=".SolarSystemListActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.mjbudden.android.infoboxes.InfoboxListActivity" />
        <activity android:name="org.mjbudden.android.infoboxes.InfoboxActivity" />
        <activity android:name="org.mjbudden.android.infoboxes.AboutActivity" />
    </application>
</manifest>
